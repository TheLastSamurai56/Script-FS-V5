            game.Players.LocalPlayer.Character.Humanoid:EquipTool(game.Players.LocalPlayer.Backpack["Bone Crush"])
			game.Workspace.Live[Player.Name]["Bone Crush"].Activator["Crash"]:Destroy()
    
