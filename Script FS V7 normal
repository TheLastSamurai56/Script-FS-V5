    function glitch1()
        local plr = game.Players.LocalPlayer
        while wait() do
            pcall(function()
            game:GetService("Workspace").Live[plr.Name].LowerTorso.KnockBacked:Destroy()
            game:GetService("Workspace").Live[plr.Name].LowerTorso.BodyVelocity:Destroy()
            game:GetService("Workspace").Live[plr.Name].LowerTorso.BodyVelocity:Destroy()

        end)
    end
    end 
